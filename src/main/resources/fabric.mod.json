{
  "schemaVersion": 1,
  "id": "${id}",
  "version": "${version}",

  "name": "${name}",
  "description": "A simple Minecraft mod that make the game window un-resizeable",
  "authors": [
    "Fallen_Breath"
  ],
  "contact": {
    "homepage": "https://modrinth.com/project/locked-window-size",
    "sources": "https://github.com/Fallen-Breath/locked-window-size"
  },

  "icon": "assets/lockedwindowsize/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "me.fallenbreath.lockedwindowsize.LockedWindowSizeMod"
    ]
  },
  "mixins": [
    "lockedwindowsize.mixins.json"
  ],

  "depends": {
    "minecraft": "${minecraft_dependency}"
  }
}
